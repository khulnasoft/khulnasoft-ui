{
  "token-selector": {
    "token-container": {
      "token": {
        "background": {
          "color": {
            "focus": {
              "$value": {
                "default": "{color.neutral.300}",
                "dark": "{color.neutral.600}"
              },
              "$type": "color",
              "$description": "Used for the background color of a token in a token-selector when it's token-container is in the focus state."
            }
          }
        }
      }
    }
  }
}
